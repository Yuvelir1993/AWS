{
  "name": "project-hub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "set NODE_ENV=prod && docusaurus start",
    "start:dev": "set NODE_ENV=dev && docusaurus start --port 3002",
    "build": "docusaurus build",
    "build:zip": "docusaurus build && powershell -Command \"Compress-Archive -Path build\\* -DestinationPath projectHubWeb.zip\"",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "set NODE_ENV=prod && docusaurus serve",
    "serve:dev": "set NODE_ENV=dev && docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "3.6.2",
    "@docusaurus/preset-classic": "3.6.2",
    "@mdx-js/react": "^3.1.0",
    "clsx": "^2.1.1",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.15"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.6.2",
    "@docusaurus/tsconfig": "3.6.2",
    "@docusaurus/types": "3.6.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "typescript": "~5.6.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}